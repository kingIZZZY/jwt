{
    "name": "hypervel/jwt",
    "description": "The jwt package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "jwt",
        "swoole",
        "hypervel"
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "jwtors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "require": {
        "php": "^8.2",
        "nesbot/carbon": "^2.72.6",
        "hyperf/collection": "3.2.x-dev",
        "lcobucci/jwt": "^5.0",
        "psr/simple-cache": "^3.0",
        "hyperf/stringable": "3.2.x-dev",
        "ramsey/uuid": "^4.7",
        "hypervel/cache": "dev-edge-hyperf3.2"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\JWT\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\JWT\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/cache"
        }
    ]
}
